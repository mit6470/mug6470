<% if @datum %>
  <table>
    <thead>
      <tr>
        <td><%= @datum.features[-1][:name] %></td>
      <% @datum.features[1...-1].each do |f| %>
        <td><%= f[:name] %></td>
      <% end %>
      </tr>
    </thead>
    <tbody>
      <% @examples && @examples.select { |e| !e.nil? }.each do |e| %>
        <tr>
          <td><%= e[-1] %></td>
          <% e[1...-1].each do |v| %>
            <td><%= v %></td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
