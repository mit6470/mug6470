<nav>
  <ol>
    <% if current_user %>
      <li>
        <%= link_to 'Projects', projects_path %>
      </li>
      <li>
        <%= link_to current_user.email, '#' %>
      </li>
      <li>
        <%= link_to 'Sign out', session_path, :method => :delete %>
      </li>
    <% else %>
      <li>
        <%= link_to 'Sign in', new_session_path %>
      </li>
      <li>
        <%= link_to 'Create account', new_user_path %>
      </li>
    <% end %>
  </ol>
</nav>
